{
  "manifest_version": 2,

  "name": "Door entry extension",
  "description": "Door entry extension",
  "version": "1.5",
  "icons": { "72": "icon.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "background",
    "gcm",
    "notifications",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}